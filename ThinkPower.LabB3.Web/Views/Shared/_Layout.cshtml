@using ThinkPower.LabB3.Web.ActionModels;
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>LabB3風險評估問卷</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div>
        <div class="layout-header">
            <nav class="navbar navbar-default navbar-fixed-top">
                <div id="nav-brand" class="navbar-header">
                    <h2>
                        @Html.ActionLink("LabB3", "AcceptRiskRank", "RiskEvaluation", new { @class = "navbar-brand" })
                    </h2>
                </div>
                <div id="nav-menu" class="navbar-right">
                    <ul class="nav navbar-nav nav-layout navbar-text">
                        <li><span>|&nbsp;@Html.ActionLink("首頁", "AcceptRiskRank", "RiskEvaluation")&nbsp;</span></li>
                        <li><span>|&nbsp;@Html.ActionLink("關於", "AcceptRiskRank", "RiskEvaluation")&nbsp;</span></li>
                        <li><span>|&nbsp;@Html.ActionLink("登出", "AcceptRiskRank", "RiskEvaluation")&nbsp;|</span></li>
                    </ul>
                </div>
            </nav>
        </div>
        <div class="layout-middle">
            <div class="layout-sidebar">
                <ul>
                    <li><a href="/RiskEvaluation/EvaQuest/?QuestId=FNDRE001"><span class="glyphicon glyphicon-arrow-right" /><span class="h4">&nbsp;風險評估問卷(1)</span></a></li>
                    <li><a href="/RiskEvaluation/EvaQuest/?QuestId=FNDRE002"><span class="glyphicon glyphicon-arrow-right" /><span class="h4">&nbsp;風險評估問卷(2)</span></a></li>
                </ul>
            </div>
            <div class="layout-main">
                @RenderBody()
            </div>
        </div>
        <footer class="layout-footer navbar-fixed-bottom">
            <h5 class="col-md-offset-1">LabB3 Inc. All Rights Reserved</h5>
        </footer>
    </div>
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)
    </body>
</html>
